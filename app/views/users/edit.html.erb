<div id="users">
<%= form_for(@user) do |f| %>
	<h4>名前</h4>
	<%= f.text_field :name %>
	<h4>自己紹介文</h4>
	<%= f.text_area :introduction %>
	<h4>経歴</h4>
	<%= f.text_field :career %>
	<h4>これからしていきたいこと</h4>
	<%= f.text_area :wants %>
	<h4>スキル</h4>
	<%= f.text_field :skill %>
	<h4>資格</h4>
	<%= f.text_field :license %>
	<h4>趣味</h4>
	<%= f.text_area :hobby %>

	<%= f.submit "この内容を保存する" %>
<% end %>
</div>